  "scripts": {
    "clean": "xs clean",
    "watch": "xs watch",
    "start": "xs serve",
    "lint": "xs lint",
    "lint:fix": "xs lint --fix",
    "types": "xs types",
    "build": "xs build",
    "screenshots": "electron scripts/screenshots",
    "screenshots:debug": "DEBUG_SCREENSHOTS=1 run-s screenshots",
    "docs": "xs docs",
    "dist": "xs build,docs",
    "postdist": "run-s screenshots copy:*",
    "copy:dist": "copyfiles -u 1 dist/* deploy",
    "copy:assets": "copyfiles \"{dist,docs,screenshots,examples,demo}/**\" deploy",
    "deploy": "xs clean,build,docs && run-s screenshots && xs upload",
    "release": "xs bump,test && run-s deploy && xs git-push",
    "publish-ci": "xs publish",
    "generate-exports": "node --no-warnings=ExperimentalWarning scripts/generate-exports.mjs"
  },
  "pre-commit": [
    "lint",
    "types"
  ],
  "files": [
    "lib",
    "dist"
  ],
  "eslintConfig": {
    "extends": "@pixi/eslint-config"
